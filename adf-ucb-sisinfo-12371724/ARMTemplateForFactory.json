			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_sql_dw_pizzas_sales",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "DW"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "pizza_id",
						"type": "varchar"
